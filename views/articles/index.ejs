<%- include ../partials/header -%>

<div class="card mb-3 article-cards">
    <% articles.slice(0,20).forEach(function(article){ %>


        <div class="card-body">
            <h5 class="card-title"><%= article.title %></h5>
            <p class="card-text"><%= article.link %></p>
        </div>

        <a href="/articles/<%=article._id%>" class="btn btn-warning stretched-link get-articles">Save</a>

        <hr>

    <% }); %>
</div>





<%- include ../partials/footer -%>